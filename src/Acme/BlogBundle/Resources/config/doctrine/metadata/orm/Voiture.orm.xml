<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Voiture" table="voiture">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="marque" type="string" column="marque" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="modele" type="string" column="modele" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nbplace" type="string" column="nbplace" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="etat" type="integer" column="etat" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
