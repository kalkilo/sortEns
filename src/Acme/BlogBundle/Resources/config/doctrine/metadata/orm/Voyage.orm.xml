<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Voyage" table="voyage">
    <id name="id" type="integer" column="id"/>
    <id name="idConducteur" type="integer" column="id_conducteur"/>
    <field name="placesTotales" type="integer" column="places_totales" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="tailleBagage" type="integer" column="taille_bagage" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="message" type="string" column="message" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="flexTemps" type="string" column="flex_temps" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="flexKm" type="string" column="flex_km" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
